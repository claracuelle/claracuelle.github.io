---
interface Props {
  class?: string
  icon: 'arrow'
      | 'check'
      | 'close'
      | 'copy'
      | 'menu'
      | 'logo'
      | 'mail'
      | 'download'
      | 'see'
}

let { class: className, icon } = Astro.props
className = `object-contain ${icon} ${className}`
---

